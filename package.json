{
  "name": "web-boostrap",
  "version": "1.0.0",
  "description": "pagina web con boostrap",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev \"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -watch -o css/ css/",
    "watch:scss": "concurrently \"onchange 'css/*.scss'\" \"npm run scss \"",
    "clean": "rimraf dist",
    "imagemin": "imagemin images/* --out-dir dist/images",
    "usemin": "usemin dist/index.html --dest dist --output dist/index.html --htmlmin true && usemin dist/contactos.html --dest dist --output dist/contactos.html --htmlmin true && usemin dist/nosotros.html --dest dist/dist --output dist/nosotros.html --htmlmin true && usemin dist/precios.html --dest dist --output dist/precios.html --htmlmin true && usemin dist/terminosycondiciones.html --dest dist --output dist/terminosycondiciones.html --htmlmin true",
    "build": "npm run clean && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://JuanSolis94@bitbucket.org/juansolis94/web-boostrap.git"
  },
  "author": "Juan Solis",
  "license": "ISC",
  "homepage": "https://bitbucket.org/juansolis94/web-boostrap#readme",
  "bugs": {
    "url": "https://bitbucket.org/juansolis94/web-boostrap/issues"
  },
  "keywords": [
    "trabajamos",
    "juntos",
    "nuestro",
    "primer",
    "boostrap"
  ],
  "devDependencies": {
    "concurrently": "^7.2.2",
    "copyfiles": "^2.4.1",
    "cssmin": "^0.4.3",
    "grunt": "^1.5.3",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^2.0.1",
    "grunt-contrib-concat": "^2.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^4.0.0",
    "grunt-contrib-imagemin": "^4.0.0",
    "grunt-contrib-sass": "^2.0.0",
    "grunt-contrib-uglify": "^5.2.2",
    "grunt-filerev": "^2.3.1",
    "grunt-usemin": "^3.1.1",
    "htmlmin": "^0.0.7",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.6.1",
    "node-sass": "^7.0.1",
    "onchange": "^7.1.0",
    "rimraf": "^3.0.2",
    "ruby": "^0.6.1",
    "time-grunt": "^2.0.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "boostrap": "^2.0.0",
    "bootstrap": "^5.1.3",
    "grunt-contrib-watch": "^1.1.0",
    "iconic": "^0.2.21",
    "imagemin": "^8.0.1",
    "jquery": "^3.6.0",
    "open-iconic": "^1.1.1",
    "popper": "^1.0.1",
    "popper.js": "^1.16.1",
    "sass": "^1.53.0"
  }
}
